<!-- Loop over each object in the projects array -->
<div class="posts">

    {{#each posts }}

    <div class="card">
        <div class="card-title">
            <h2>
                <a href="/post/{{id}}">{{title}}</a>
            </h2>
            <p>Created by {{User.username}} on {{format_date createdAt}}</p>
        </div>
    </div>

    {{/each}}
</div>